{
  "name": "cec-ide",
  "displayName": "CEC-IDE",
  "description": "国产化你的VSCode,来源于CEC-IDE,有敏感词检测功能,这下自主创新了!",
  "publisher": "qcqx",
  "repository": {
    "type": "git",
    "url": "https://github.com/qxchuckle/vsc-cec-ide.git"
  },
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onUriOpen"
  ],
  "main": "./out/extension.js",
  "icon": "resource/images/CEC-IDE.png",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cec-ide",
          "title": "CEC-IDE",
          "icon": "./resource/images/CEC-IDE.svg"
        }
      ]
    },
    "views": {
      "cec-ide": [
        {
          "id": "cec-sidebar-main",
          "name": "主界面",
          "icon": "./resource/images/CEC-IDE.svg",
          "type": "webview",
          "contextualTitle": "主界面"
        }
      ]
    },
    "commands": [
      {
        "command": "cec-ide.cec-ide",
        "title": "CEC-IDE"
      },
      {
        "command": "cec-ide.cec-ide-restore",
        "title": "CEC-IDE-RESTORE"
      },
      {
        "command": "cec-ide.mark-sensitive-words",
        "title": "检测敏感词"
      },
      {
        "command": "cec-ide.stop-mark-sensitive-words",
        "title": "停止检测敏感词"
      },
      {
        "command": "cec-ide.uploadSensitiveWordsFile",
        "title": "CEC-UploadSensitiveWordsFile"
      },
      {
        "command": "cec-ide.resetSensitiveWordsFile",
        "title": "CEC-ResetSensitiveWordsFile"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "cec-ide.mark-sensitive-words",
          "group": "navigation@1"
        },
        {
          "when": "editorTextFocus",
          "command": "cec-ide.stop-mark-sensitive-words",
          "group": "navigation@1"
        }
      ]
    },
    "keybindings": [
      {
        "command": "cec-ide.mark-sensitive-words",
        "key": "alt+shift+m",
        "mac": "alt+shift+m",
        "when": "editorTextFocus"
      },
      {
        "command": "cec-ide.stop-mark-sensitive-words",
        "key": "alt+shift+n",
        "mac": "alt+shift+n",
        "when": "editorTextFocus"
      }
    ],
    "filters": {
      "textFiles": [
        "txt"
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "mint-filter": "^4.0.3"
  }
}
